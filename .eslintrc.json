{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-undef": "error",
    "no-redeclare": "error",
    "no-dupe-keys": "error",
    "semi": ["warn", "always"],
    "quotes": "off",
    "indent": "off"
  },
  "globals": {
    "dayjs_plugin_relativeTime": "readonly",
    "dayjs_plugin_localizedFormat": "readonly"
  }
}
